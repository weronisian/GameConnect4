                        <Insets bottom="20.0" left="20.0" />
                     </VBox.margin>
                  </Text>
                  <RadioButton fx:id="radio1_button" accessibleRole="RADIO_MENU_ITEM" mnemonicParsing="false" selected="true" text="Human vs Human">
                     <font>
                        <Font size="20.0" />
                     </font>
                     <padding>
                        <Insets bottom="10.0" left="20.0" />
                     </padding>
                  </RadioButton>
                  <RadioButton fx:id="radio2_button" accessibleRole="RADIO_MENU_ITEM" mnemonicParsing="false" text="Human vs AI">
                     <font>
                        <Font size="20.0" />
                     </font>
                     <padding>
                        <Insets bottom="10.0" left="20.0" />
                     </padding>
                  </RadioButton>
                  <RadioButton fx:id="radio3_button" accessibleRole="RADIO_MENU_ITEM" mnemonicParsing="false" text="AI vs AI">
                     <font>
                        <Font size="20.0" />
                     </font>
                     <padding>
                        <Insets bottom="10.0" left="20.0" />
                     </padding>
                  </RadioButton>
                  <ComboBox fx:id="depth_combobox" prefWidth="200.0" promptText="Select depth">
                     <VBox.margin>
                        <Insets bottom="20.0" left="20.0" top="20.0" />
                     </VBox.margin>
                  </ComboBox>
                  <ComboBox fx:id="function_combobox" prefWidth="200.0" promptText="Select function">
                     <VBox.margin>
                        <Insets bottom="10.0" left="20.0" top="10.0" />
                     </VBox.margin>
                  </ComboBox>
                  <Text fx:id="game_mode_text" strokeType="OUTSIDE" strokeWidth="0.0" text="Game Human vs Human" visible="false">
                     <font>
                        <Font size="24.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="30.0" left="20.0" top="20.0" />
                     </VBox.margin>
                  </Text>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="start_button" cancelButton="true" mnemonicParsing="false" text="Start" textFill="#8c1c1c">
                           <font>
                              <Font size="25.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="30.0" right="20.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="restart_button" mnemonicParsing="false" text="Restart" textFill="#941a1a">
                           <font>
                              <Font size="25.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="20.0" right="20.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                     <effect>
                        <DropShadow />
                     </effect>
                  </HBox>
                  <Text fx:id="win_text" strokeType="OUTSIDE" strokeWidth="0.0" text="Wygrał" visible="false">
                     <font>
                        <Font size="22.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin>
                  </Text>
                  <Text fx:id="error_text" fill="#e50000" strokeType="OUTSIDE" strokeWidth="0.0" text="Błąd" visible="false">
                     <font>
                        <Font size="22.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin>
                  </Text>
               </children>
            </VBox>
         </children>
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
      </Pane>
